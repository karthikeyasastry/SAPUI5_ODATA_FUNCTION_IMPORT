<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="valueHelpDialog"
		title="Plants"
		items="{/I_PlantStdVH}"
		search="_handleValueHelpSearch"
		confirm="_handlePlantValueHelpClose"
		cancel="_handlePlantValueHelpClose"
		multiSelect="true">
		<StandardListItem
			title="{Plant}"
			description="{PlantName}" />
	</SelectDialog>
</core:FragmentDefinition>